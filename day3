height = input("Hello! What is your height in centimeters?: ")
price = 0

if int(height) >= 120:
  age = input("Secondly, what is your age?: ")
  
  if int(age) >= 45 and int(age) <= 55:
    print("Since you have been driven out on a new inner journey, here is a free ticket!")
    price = 0
  elif int(age) >= 18:
    print("Adult tickets are 12$")
    price = 12
  elif int(age) <18 and int(age) >= 12:
    print("Youth tickets are 7$")
    price = 7
  else: 
    print("Child tickets are 5$")
    price = 5

  photo = input("Would you like a photo? Y or N:")
  if photo == "Y":
    price += 3
  print(f"Your total is ${price}")
  
else:
  print("You are not tall enough to enter this exquisit ride.")
